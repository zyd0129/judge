package com.ps.judge;

import com.ps.judge.provider.entity.ScoreCardVO;
import com.ps.jury.api.response.level.MerchantLevel;
import com.ps.jury.api.response.group.DeviceInfo;

rule "DE0006_02"
	dialect "mvel"
	ruleflow-group "CS01"
	when
		$scoreCard : ScoreCardVO( $score : score)
		MerchantLevel( deviceInfo != null , deviceInfo.mobileBrand in ( "oneplus", "realme", "karbonn" ) )
	then
		$scoreCard.setScore( $score + 10 );
end
