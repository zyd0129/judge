package com.ps.jury.api.objects.judge;

import com.ps.judge.provider.entity.ScoreCardVO;
import com.ps.jury.api.response.level.MerchantLevel;
import com.ps.jury.api.response.group.DeviceInfo;

rule "DE0006_05"
	dialect "mvel"
	ruleflow-group "CS01"
	when
		$scoreCard : ScoreCardVO( $score : score)
		MerchantLevel( deviceInfo != null , deviceInfo.mobileOsVersion in ( "google", "htc", "huawei", "letv", "tecno" ) )
	then
		$scoreCard.setScore( $score - 5 );
end
