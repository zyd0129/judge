<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ps.judge.dao.mapper.ConfigFlowRulePackageMapper">
    <resultMap id="resultMap" type="com.ps.judge.dao.entity.ConfigFlowRulePackageDO">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="flow_code" property="flowCode" jdbcType="VARCHAR"/>
        <result column="rule_package_version_id" property="rulePackageVersionId" jdbcType="INTEGER"/>
        <result column="sort" property="sort" jdbcType="TINYINT"/>
        <result column="gmt_created" property="gmtCreated" jdbcType="TIMESTAMP"/>
        <result column="gmt_modified" property="gmtModified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <select id="listConfigFlowRulePackageByFlowCode" resultMap="resultMap">
        select * from config_flow_rule_package  where flow_code = #{flowCode}
    </select>

</mapper>