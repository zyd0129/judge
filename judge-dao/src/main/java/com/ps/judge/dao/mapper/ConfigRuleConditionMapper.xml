<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ps.judge.dao.mapper.ConfigRuleConditionMapper">
    <resultMap id="resultMap" type="com.ps.judge.dao.entity.ConfigRuleConditionDO">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="rule_id" property="ruleId" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="operator" property="operator" jdbcType="VARCHAR" />
        <result column="operand" property="operand" jdbcType="VARCHAR" />
        <result column="function" property="function" jdbcType="VARCHAR" />
        <result column="variable_code" property="variableCode" jdbcType="VARCHAR" />
        <result column="variable_type" property="variableType" jdbcType="INTEGER" />
        <result column="status" property="status" jdbcType="INTEGER" />
        <result column="gmt_created" property="gmtCreated" jdbcType="TIMESTAMP" />
        <result column="gmt_modified" property="gmtModified" jdbcType="TIMESTAMP" />
    </resultMap>

    <select id="getConfigRuleCondition" resultMap="resultMap">
        select * from config_rule_condition where rule_id = #{ruleId}
    </select>

</mapper>